,[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/account-list/account-list.data.js",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/pages/account/account.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/account-list-item/account-list-item.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/account-list/account-list.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/settings/settings.component.jsx",["277"],"import React, { createRef } from 'react';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faTrashAlt } from '@fortawesome/free-solid-svg-icons';\nimport UserInput from '../user-input/user-input.component';\nimport UserButton from '../user-button/user-button.component';\nimport axios from 'axios';\n\nimport './settings.styles.scss';\n\nclass Settings extends React.Component {\n    constructor() {\n        super();\n        this.state = {\n            nameInput: '',\n            surnameInput: '',\n            emailInput: '',\n            phoneNoInput: '',\n            isLoadedPhoto: false,\n            currentUser:\n                JSON.parse(localStorage.getItem('currentUser')) || null,\n        };\n        this.fileRef = createRef();\n        this.photoForm = createRef();\n        this.photoRef = createRef();\n    }\n\n    componentDidMount() {\n        if (this.state.currentUser) {\n            const { name, email } = this.state.currentUser;\n            this.setState({\n                nameInput: name,\n                emailInput: email,\n            });\n        }\n    }\n\n    openFileDialog = async () => {\n        const file = this.fileRef.current;\n\n        if (!this.state.isLoadedPhoto) {\n            file.click();\n        } else {\n            if (file.files[0]) {\n                const formData = new FormData();\n                formData.append('coverPhoto', file.files[0]);\n\n                await this.sendUserChanges(formData, {\n                    'Content-Type': 'multipart/form-data',\n                });\n\n                file.value = null;\n            }\n        }\n    };\n\n    sendUserChanges = async (data, headers) => {\n        let user = await axios({\n            method: 'PATCH',\n            url: `/api/users/updateMe`,\n            data,\n            headers,\n        });\n\n        if (user) {\n            user = user.data.data.data;\n            this.setState(\n                {\n                    currentUser: user,\n                    isLoadedPhoto: false,\n                },\n                () => {\n                    localStorage.setItem('currentUser', JSON.stringify(user));\n                }\n            );\n        }\n    };\n\n    photoHandler = (event) => {\n        event.preventDefault();\n        const { files } = event.target;\n\n        const isLoadedPhoto = event.target.files.length > 0 ? true : false;\n        this.setState({ isLoadedPhoto });\n    };\n\n    inputHandler = (event) => {\n        const { name, value } = event.target;\n        this.setState({\n            [name]: value,\n        });\n    };\n\n    resetPhoto = async (event) => {\n        event.preventDefault();\n        const { isLoadedPhoto, currentUser } = this.state;\n        if (isLoadedPhoto) {\n            this.fileRef.current.value = null;\n            this.photoRef.current.src = `/uploads/users/${\n                currentUser.photo ? currentUser.photo : 'default.png'\n            }`;\n            this.setState({\n                isLoadedPhoto: false,\n            });\n        } else {\n            await this.sendUserChanges({ photo: 'default.png' }, {});\n        }\n    };\n\n    render() {\n        const { currentUser } = this.state;\n        return (\n            <div className=\"account-container\">\n                <div className=\"user-photo-container\">\n                    <img\n                        src={`/uploads/users/${\n                            currentUser.photo\n                                ? currentUser.photo\n                                : 'default.png'\n                        }`}\n                        alt=\"\"\n                        ref={this.photoRef}\n                    />\n                    <div className=\"user-photo-buttons-wrapper\">\n                        <UserButton onClick={this.openFileDialog}>\n                            {this.state.isLoadedPhoto ? 'Zapisz' : 'Zmień'}\n                        </UserButton>\n                        <UserButton\n                            additionalClass=\"trash-btn\"\n                            onClick={this.resetPhoto}\n                        >\n                            {this.state.isLoadedPhoto ? (\n                                'Reset'\n                            ) : (\n                                <FontAwesomeIcon icon={faTrashAlt} />\n                            )}\n                        </UserButton>\n                    </div>\n                </div>\n                <form\n                    className=\"photo-form\"\n                    ref={this.photoForm}\n                    encType=\"multipart/form-data\"\n                >\n                    <input\n                        type=\"file\"\n                        ref={this.fileRef}\n                        onChange={this.photoHandler}\n                        hidden\n                    />\n                </form>\n                <form className=\"user-data-form\">\n                    <UserInput\n                        type=\"text\"\n                        value={this.state.nameInput}\n                        onChange={this.inputHandler}\n                        name=\"nameInput\"\n                    >\n                        Imię:\n                    </UserInput>\n                    <UserInput\n                        type=\"text\"\n                        value={this.state.emailInput}\n                        onChange={this.inputHandler}\n                        name=\"emailInput\"\n                    >\n                        Email:\n                    </UserInput>\n                    <UserButton>Zapisz zmiany</UserButton>\n                </form>\n            </div>\n        );\n    }\n}\n\nexport default Settings;\n","/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/user-input/user-input.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/change-password/change-password.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/user-button/user-button.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/account-directory/account-directory.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/alert-basic/alert-basic.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/account-list-mobile/account-list-mobile.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/address-viewer/address-viewer.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/address-card/address-card.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/address-card/address-card.validator.js",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/address-input/address-input.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/address-viewer/emptyAddress.js",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/userStorageManager/userStorageManager.js",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/components/pizza-preview-box/pizza-preview-box.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-portal/pizza-portal/src/pages/pizza-creator/pizza-creator.component.jsx",[],"/home/mateusz/Pulpit/projekty/pizza-port